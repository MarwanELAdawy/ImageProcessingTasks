img = imread('Moire1.bmp');
I = rgb2gray(img);
I = im2double(I);
figure , imshow(I,[]),title("GrayImg");
fouri = fft2(I);
figure , imshow(fouri,[]),title("Fourir Img");
shiftImg = fftshift(fouri);
figure , imshow(shiftImg,[]),title("Shiftted Img");
LogImg = log(abs(shiftImg));
figure , imshow(LogImg,[]),title("Log Img");
